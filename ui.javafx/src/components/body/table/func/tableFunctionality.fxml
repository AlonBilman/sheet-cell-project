<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<ScrollPane fx:id="scrollPane" fitToHeight="true" fitToWidth="true" prefHeight="401.0" prefWidth="225.0" stylesheets="@tableFunctionalityDefault.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.body.table.func.TableFunctionalityController">
   <content>
      <VBox fx:id="tableFuncVBox" alignment="TOP_CENTER" minHeight="700.0" minWidth="106.0" spacing="10.0">
         <children>
            <VBox alignment="CENTER" spacing="10.0" VBox.vgrow="SOMETIMES">
               <children>
                  <Label text="Table controls" underline="true">
                     <font>
                        <Font name="Baskerville Old Face" size="18.0" />
                     </font>
                  </Label>
                  <Button id="ColSetButton" fx:id="setColButton" disable="true" mnemonicParsing="false" onAction="#setColActionListener" prefHeight="40.0" prefWidth="150.0" text="Set Column Width" underline="true" />
                  <Button id="RowSetButton" fx:id="setRowButton" disable="true" mnemonicParsing="false" onAction="#setRowActionListener" prefHeight="40.0" prefWidth="150.0" text="Set Row Height" underline="true" />
                  <Button fx:id="alignmentSetButton" disable="true" mnemonicParsing="false" onAction="#alignmentSetListener" prefHeight="40.0" prefWidth="150.0" text="Alignment Set" underline="true" />
                  <Label text="Cell styling controls" underline="true">
                     <font>
                        <Font name="Baskerville Old Face" size="18.0" />
                     </font>
                  </Label>
                  <Button fx:id="cellBackgroundPick" disable="true" mnemonicParsing="false" onAction="#cellBackgroundColorPick" prefHeight="40.0" prefWidth="150.0" text="Cell background color" />
                  <Button fx:id="cellTextPick" disable="true" mnemonicParsing="false" onAction="#cellTextColorPick" prefHeight="40.0" prefWidth="150.0" text="Cell text color" underline="true" />
                  <Button fx:id="resetButton" disable="true" mnemonicParsing="false" onAction="#resetCellStyleListener" prefHeight="40.0" prefWidth="150.0" text="Reset Style" textAlignment="CENTER" underline="true" />
                  <Label text="Sort and Filter" underline="true">
                     <font>
                        <Font name="Baskerville Old Face" size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </Label>
                  <Button fx:id="filterButton" disable="true" mnemonicParsing="false" onAction="#filterButtonListener" prefHeight="40.0" prefWidth="150.0" text="Filter" />
                  <Button fx:id="sortButton" disable="true" mnemonicParsing="false" onAction="#sortButtonListener" prefHeight="40.0" prefWidth="150.0" text="Sort" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="82.0" prefWidth="143.0" spacing="10.0" VBox.vgrow="SOMETIMES">
               <children>
                  <Label text="Range controls" underline="true">
                     <font>
                        <Font name="Baskerville Old Face" size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </Label>
                  <Button fx:id="addNewRangeButton" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#addNewRangeListener" prefHeight="40.0" prefWidth="150.0" text="Add Range Set">
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="deleteExistingRangeButton" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#deleteExistingRangeListener" prefHeight="40.0" prefWidth="150.0" text="Delete Range" />
                  <Button fx:id="viewExistingRangeButton" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#viewExistingRangeListener" prefHeight="40.0" prefWidth="150.0" text="View Range" />
               </children>
            </VBox>
         </children>
      </VBox>
   </content>
</ScrollPane>
