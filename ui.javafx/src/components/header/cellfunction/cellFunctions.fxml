<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="56.0" prefWidth="820.0" spacing="15.0" stylesheets="@cellFunctions.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.header.cellfunction.CellFunctionsController">
   <children>
      <Label id="CellIDLabel" fx:id="cellIdProperty" maxWidth="105.0" prefHeight="56.0" prefWidth="97.0" style="-fx-border-color: blue;" text="Selected Cell ID" textAlignment="CENTER">
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin>
      </Label>
      <Label id="OriginalCellValueLabel" fx:id="cellValueProperty" maxHeight="56.0" maxWidth="305.0" prefHeight="56.0" prefWidth="294.0" style="-fx-border-color: blue;" text="Original Cell Value" />
      <VBox prefHeight="51.0" prefWidth="151.0">
         <children>
            <Button id="UpdateNewCellValueButton" fx:id="updateCellButton" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#updateCellActionListener" prefHeight="25.0" prefWidth="214.0" text="Update Cell" underline="true" />
            <TextField fx:id="newOriginalValText" alignment="CENTER" disable="true" prefHeight="25.0" prefWidth="162.0" text="new original val" />
         </children>
      </VBox>
      <Label id="LastUpdatedVersionLabel" fx:id="cellUpdatedProperty" maxHeight="58.0" maxWidth="137.0" prefHeight="58.0" prefWidth="84.0" style="-fx-border-color: blue;" text="version" />
      <Button id="VersionPickerButton" fx:id="versionPickerButton" disable="true" prefHeight="50.0" prefWidth="129.0" onAction="#getVersionListener" text="Versions:">
         <HBox.margin>
            <Insets right="7.0" top="3.0" />
         </HBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="10.0" top="15.0" />
   </padding>
</HBox>