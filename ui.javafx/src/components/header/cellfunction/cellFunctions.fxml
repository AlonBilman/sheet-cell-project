<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="53.0" spacing="15.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.header.cellfunction.CellFunctionsController">
   <children>
      <TextArea id="CellIDTextBox" fx:id="cellIdProperty" disable="true" editable="false" prefWidth="100.0" promptText="Selected Cell ID">
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin>
      </TextArea>
      <TextArea id="OriginalCellValueTextBox" fx:id="cellValueProperty" disable="true" editable="false" prefHeight="38.0" prefWidth="114.0" promptText="Original Cell Value" />
      <Button id="UpdateNewCellValueTextBox" fx:id="updateCellButton" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#updateCellActionListener" styleClass="WaitForFileLoad" text="Update Cell" underline="true">
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </Button>
      <TextArea id="LastUpdatedVersionTextBox" fx:id="cellUpdatedProperty" disable="true" prefHeight="38.0" prefWidth="133.0" promptText="Last updated cell version" />
      <ChoiceBox id="VersionPickerChoiceBox" fx:id="versionPickerChoiceBox" disable="true" prefHeight="25.0" prefWidth="98.0" styleClass="WaitForFileLoad">
         <HBox.margin>
            <Insets right="7.0" top="3.0" />
         </HBox.margin>
      </ChoiceBox>
   </children>
   <padding>
      <Insets bottom="10.0" top="15.0" />
   </padding>
</HBox>
