<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.header.cellFunction.CellFunctionsController">
   <children>
      <HBox prefHeight="53.0" spacing="15.0">
         <children>
            <Label id="CellIDTextBox" prefHeight="38.0" prefWidth="114.0" text="Cell ID">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Label>
            <Label id="OriginalCellValueTextBox" prefHeight="38.0" prefWidth="114.0" text="Original cell value" />
            <Button id="UpdateNewCellValueTextBox" fx:id="updateCellButton" alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#updateCellActionListener" styleClass="WaitForFileLoad" text="Update Cell" underline="true">
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
            </Button>
            <Label id="LastUpdatedVersionTextBox" prefHeight="38.0" prefWidth="133.0" text="Last updated version" />
            <ChoiceBox id="VersionPickerChoiceBox" fx:id="versionPickerChoiceBox" disable="true" prefHeight="25.0" prefWidth="98.0" styleClass="WaitForFileLoad">
               <HBox.margin>
                  <Insets right="7.0" top="3.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
         <padding>
            <Insets bottom="10.0" top="15.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
