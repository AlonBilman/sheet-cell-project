<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="17.0" prefWidth="1110.0" stylesheets="@titleCardTheme2.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="components.header.title.TitleCardController">
    <children>
        <Button fx:id="backToMainScreenButton" layoutX="987.0" layoutY="12.0" mnemonicParsing="false" onAction="#backToMainScreenButtonListener" text="Back to Main-&gt;" />

        <HBox layoutX="341.0" layoutY="5.0" prefHeight="38.0" prefWidth="502.0">
            <children>
                <!-- "Sheet Cell" text with specific fx:id -->
                <Text fx:id="sheetCellText" strokeType="OUTSIDE" strokeWidth="0.0" text="Sheet Cell - logged as :" underline="true" wrappingWidth="273.0">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Text>
                <Label fx:id="nameLabel" prefHeight="38.0" prefWidth="162.0">
                    <font>
                        <Font size="25.0" />
                    </font>
                </Label>
            </children>
        </HBox>

        <Button fx:id="reloadSheetButton" disable="true" layoutX="862.0" layoutY="12.0" mnemonicParsing="false" onAction="#reloadSheetListener" prefHeight="29.0" prefWidth="109.0" text="Load Update" />
    </children>
</AnchorPane>
